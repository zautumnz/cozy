let y = fn () {
    # array_from is a global to gather elements into an array
    print(util.array_from(...))
}
y(1, 2, 3)

# using the .... spread operator, which is the opposite of array_from
let whatever = fn () {
    print(...)
}
let thing = fn () {
    let gathered = util.array_from(...)
    return whatever(....gathered)
}
thing(1, 2, 3)
